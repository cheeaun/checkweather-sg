{
  "name": "checkweather-sg",
  "version": "2.0.0",
  "description": "Yet another weather app for Singapore",
  "scripts": {
    "parcel": "parcel",
    "start": "parcel index.html privacy-policy/index.html",
    "build": "rm -rf dist && parcel build index.html privacy-policy/index.html --no-cache",
    "build-prod": "npm run build && cpy {CNAME,favicon.ico} dist"
  },
  "keywords": [
    "weather",
    "rain",
    "rainfall",
    "singapore",
    "map"
  ],
  "author": "Lim Chee Aun",
  "license": "MIT",
  "dependencies": {
    "@turf/helpers": "~6.1.4",
    "d3-contour": "~1.3.2",
    "firebase": "~7.15.0",
    "mapbox-gl": "~1.10.1",
    "nano-memoize": "~1.1.11",
    "preact": "~10.4.4",
    "react-use": "~15.2.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx": "~7.10.1",
    "autoprefixer": "~9.8.0",
    "cpy-cli": "~3.1.1",
    "parcel": "~2.0.0-alpha.3.2"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom": "preact/compat"
  }
}
